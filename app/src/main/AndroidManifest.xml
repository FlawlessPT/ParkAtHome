<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pt.park_at_home.parkathome">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".MainLogin">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pt.park_at_home.parkathome.MainLogin" />
        </activity>
        <activity
            android:name=".Parking"
            android:label="@string/title_activity_parking"
            android:parentActivityName=".MainLogin"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pt.park_at_home.parkathome.MainLogin" />
        </activity>
    </application>

</manifest>